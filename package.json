{
    "name": "lokalise-test-tasks",
    "jest": {
        "testEnvironment": "./e2e-jest-environment.js",
        "setupFiles": [
            "./test/jest.overrides.js"
        ],
        "setupFilesAfterEnv": [
            "./e2e-jest-additional"
        ],
        "roots": [
            "<rootDir>/test/"
        ],
        "testMatch": [
            "<rootDir>/test/*.js?(x)"
        ]
    },
    "version": "0.0.1",
    "description": "Test automation framework using jest and puppeteer",
    "repository": {
        "type": "git",
        "url": "https://github.com/eugenepolschikov/puppeteer_test_task_202105"
    },
    "main": "server.js",
    "scripts": {
        "test-dev": "jest --runInBand test/e2e-regression.spec --e dev",
        "test-latestcommit": "jest --runInBand --expand test/e2e-regression.spec latestCommit",
        "eslint": "eslint --ext js,vue .",
        "prettier": "prettier --list-different --write \"**/*.{js,vue}\" ; echo \"All files listed above, if any, have been automatically formatted by Prettier.\""
    },
    "pre-commit": [
        "eslint"
    ],
    "author": {
        "name": "Oliver Wyman Labs",
        "email": "OWLabsSWATTeam@oliverwyman.com",
        "url": "http://http://labs.oliverwyman.com/"
    },
    "license": "MIT",
    "dependencies": {
        "artillery": "^1.6.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.8.0",
        "babel-preset-env": "^1.7.0",
        "bluebird": "^3.7.2",
        "chai": "^4.1.2",
        "config": " ^1.30.0",
        "dotenv": "^8.2.0",
        "eslint": "5.13.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-config-prettier": "4.0.0",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-prettier": "3.0.1",
        "faker": "^4.1.0",
        "jest": "^24.8.0",
        "jest-allure": "^0.1.3",
        "jest-image-snapshot": "^3.0.1",
        "minimist": "^1.2.5",
        "moment": "^2.24.0",
        "path": "^0.12.7",
        "prettier": "1.16.4",
        "puppeteer": "8.0.0",
        "puppeteer-page-object": "^2.2.0",
        "datejs": "^1.0.0-rc3"
    },
    "devDependencies": {},
    "engines": {
        "node": ">=10.15.3"
    }
}
